<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.agenda"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ekpa"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.agendaMain.AgendMainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.news.ITCutiesReaderAppActivity"
            android:label="@string/title_activity_display_news" >
        </activity>
		<activity
            android:name="com.example.courses.Courses"
            android:label="@string/title_activity_display_courses" >
        </activity>
        <activity
            android:name="com.example.profile.Profile"
            android:label="@string/title_activity_display_profile" >
        </activity>
        <activity
            android:name="com.example.scedule.Schedule"
            android:label="@string/title_activity_display_schedule" >
        </activity>
        <activity
            android:name="com.example.agenda.MainActivity"
            android:label="@string/title_activity_main" >
        </activity>

    </application>

</manifest>